    TextOptions.TextFormattingMode="Ideal"
    TextOptions.TextRenderingMode="Auto"
    mc:Ignorable="d">

    <Grid Background="Aqua">
        <TextBlock FontSize="150">Ví tiền</TextBlock>
    </Grid>
</Page>
